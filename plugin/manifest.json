{
	"manifest_version": 2,

	"name": "bezier",
	"version": "0.0.1",
	"description": "convert dom to image for generating bezier curve",
	"author": "gaoxing",

	"browser_action": {
		"default_icon": "icon.png"
	},

	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},

	"background": {
		"scripts": ["event.js"]
	},

	"permissions": ["activeTab", "tabs"],
	"web_accessible_resources": ["template.html"]
}
